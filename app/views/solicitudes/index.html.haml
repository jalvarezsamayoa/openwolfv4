- content_for :head do
  = javascript_include_tag 'jquery-blockui.js','solicitudes.js', 'seguimientos.js'
= render :partial => "breadcrums"
%h1.alt Solicitudes y Tareas
.solicitudes-y-tareas
  #solicitudes-y-tareas-tabs
    %ul
      %li= link_to "Tareas Asignadas (#{@asignaciones.size})", "#tareas", :title => "Tareas que debo de completar."
