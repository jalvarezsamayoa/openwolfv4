- content_for :head do
  = javascript_include_tag 'jquery-blockui.js', 'solicitudes', 'jquery.ui.datepicker-es.js'
- @forview = "EDIT"
= render :partial => "breadcrums"
.mantform
  %h2
    Editar #{Solicitud.human_name}
  - semantic_form_for @solicitud, :html => {:class => "edit_solicitud"} do |form|
    = form.semantic_errors
    = render :partial => 'form', :locals => {:form => form}
    = buttons_update(solicitud_path(@solicitud))