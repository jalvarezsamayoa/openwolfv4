#page-title.clearfix
  %h1= "Nueva #{Via.model_name.human}"
  = render :partial => "shared/breadcrumbs"

.show.widget
  .widget-header
    %h3 Nueva Via
  .widget-content
    = simple_form_for([:admin,@via], :html => {:class => 'form-horizontal'}) do |form|
      = render :partial => 'form', :locals => {:form => form}
      .form-actions
        = buttons_create(admin_vias_path)