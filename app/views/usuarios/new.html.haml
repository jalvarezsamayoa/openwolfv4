- content_for :head do
  = javascript_include_tag 'combobox.js', 'usuarios.js'
- @forview = "NEW"
= render :partial => "breadcrums"
.mantform
  %h2
    Nuevo #{Usuario.human_name}
  - semantic_form_for @usuario do |form|
    = form.semantic_errors
    = render :partial => 'form', :locals => {:form => form}
    = buttons_create(usuarios_path)
